{
  "name": "ng-fveh-app",
  "author": {
    "name": "Guilherme Waess",
    "email": "guilherme.waess@gmail.com"
  },
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build-app": "ng build --project=ng-fveh-app --aot",
    "build-app-prod": "ng build --project=ng-fveh-app --prod --base-href=https://guilhermewaess.github.io/ng-fveh/",
    "build-lib": "ng build --project=ng-fveh",
    "build-lib-prod": "ng build --project=ng-fveh --prod",
    "test-app": "ng test --project=ng-fveh-app",
    "test-lib": "ng test --project=ng-fveh",
    "test-lib-ci": "ng test --project=ng-fveh --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-app": "npm run build-app-prod && git subtree push --prefix dist/ng-fveh-app origin gh-pages",
    "publish-lib": "npm run build-lib-prod && npm run npm-pack",
    "npm-pack": "cd dist/ng-fveh && npm pack && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cdk": "^6.4.0",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/material": "^6.4.0",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "core-js": "^2.5.4",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/sinon": "^5.0.1",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "sinon": "^6.1.3",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  }
}
